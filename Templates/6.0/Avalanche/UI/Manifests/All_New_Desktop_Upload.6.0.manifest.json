          {
            "name": "processing-details",
            "label": "Select Processing Details",
            "class": "wizard-header-with-icon",
            "iconImage": "./assets/images/icons/Refresh-Grey-Background.svg",
            "iconClass": "pr-2",
            "wizardClass": "wizard-warpper-content",
